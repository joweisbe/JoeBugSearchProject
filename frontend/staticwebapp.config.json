{
  "routes": [
    {
      "route": "/api/*",
      "methods": ["GET", "POST"],
      "rewrite": "/api/{restOfPath}"
    },
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/api/*"]
  },
  "mimeTypes": {
    ".js": "application/javascript",
    ".jsx": "application/javascript"
  },
  "globalHeaders": {
    "Cache-Control": "no-store, must-revalidate, no-cache, max-age=0",
    "Content-Type": "text/jsx",
    "X-Content-Type-Options": "nosniff"
  }
}


